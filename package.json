{
  "name": "taskcluster-client-web",
  "version": "8.1.0",
  "main": "build/index.js",
  "author": "Eli Perelman <eli@eliperelman.com>",
  "license": "MPL-2.0",
  "repository": "taskcluster/taskcluster-client-web",
  "files": [
    "src",
    "build"
  ],
  "scripts": {
    "build": "webpack --mode production",
    "docs": "rm -f docs/* && node -r dotenv/config bin/generate-docs",
    "compile-clients": "rm -f src/clients/* && node -r dotenv/config bin/compile-clients && yarn lint --fix",
    "lint": "eslint --cache --format codeframe --ext js src",
    "list-clients": "node -r dotenv/config bin/list-clients",
    "prepare": "yarn build",
    "show-client": "node -r dotenv/config bin/show-client",
    "test": "karma start --single-run",
    "update-all": "yarn compile-clients && yarn build && yarn docs"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.2.1",
    "@mozilla-frontend-infra/node-lint": "^1.1.1",
    "@neutrinojs/karma": "^9.0.0-rc.0",
    "@neutrinojs/library": "^9.0.0-rc.0",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "cliff": "^0.1.10",
    "common-tags": "^1.4.0",
    "dotenv": "^6.0.0",
    "ejs": "^2.5.7",
    "eslint": "^5.10.0",
    "got": "^8.3.1",
    "javascript-stringify": "^1.6.0",
    "json-schema-to-markdown": "^1.0.3",
    "json-stable-stringify": "^1.0.1",
    "karma": "^3.1.4",
    "karma-cli": "^2.0.0",
    "karma-firefox-launcher": "^1.1.0",
    "karma-sourcemap-loader": "^0.3.7",
    "mocha": "^5.2.0",
    "neutrino": "^9.0.0-rc.0",
    "object.omit": "^3.0.0",
    "webpack": "^4.27.1",
    "webpack-cli": "^3.1.2"
  },
  "dependencies": {
    "hawk": "^7.0.7",
    "query-string": "^6.1.0",
    "taskcluster-lib-urls": "^10.0.0"
  }
}
